{
    "problemMatcher": [
        {
            "owner": "node",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+)\\s(.+)\\s\\^\\s\\sAssertionError \\[ERR_ASSERTION\\]: (.+\\s\\s.+)\\s\\s    ",
